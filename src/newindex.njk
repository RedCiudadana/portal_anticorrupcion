                                <img src="/ELEMENTOS/ELEMENTOS MEJORAS/NEWICONS/ICONOS-50.png" alt="" />
                            </div>
                            <div class="banner-text">
                                <h3 class="icon-box-title">{{item.data.instituciones.name}}</h3>
                            </div>
                        </div></a>
                    </div>
                    {% endfor %}
                    
