                                            {%endif%}
                                            {% if categoria2.id == "28" %}
                                                <img class="default-img" src="/ELEMENTOS/ELEMENTOS MEJORAS/NEWICONS/ICONOS-35.png" alt="" />
                                            {%endif%}
                                            {% if categoria2.id == "29" %}
                                                <img class="default-img" src="/ELEMENTOS/ELEMENTOS MEJORAS/NEWICONS/ICONOS-47.png" alt="" />
