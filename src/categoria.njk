---
pagination:
  data: categorias
  size: 1
  alias: categoria2
  addAllPagesToCollections: true
permalink: "categorias/{{categoria2.name | slug}}/index.html"
tags:
  - categoria2
---
{% extends "src/base.njk" %}

{% block head %}
  <title>Catálogo de Trámites | {{categoria2.name}}</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
{% endblock %}

{% block content %}
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="/index2.html" rel="nofollow"><i class="fi-rs-home mr-5"></i>Inicio</a>
                    <span></span> {{categoria2.name}}
                </div>
            </div>
        </div>
        <div class="page-content pt-50">
            <div class="container">
                <div class="archive-header-2 text-center">
                    <h1 class="heading-2 mb-10">{{categoria2.name}}</h1>
                    {% set cuenta = 0 %}
                    {% for catesp in collections.categoriaesp2 %}
                    {% if categoria2.id == catesp.data.categoriaesp2.category | lastSlash %}
                    {% for servicio in collections.servicios%}
                    {% if catesp.data.categoriaesp2.name == servicio.data.servicio.subcategory.name%}
                    {% set cuenta = cuenta + 1 %}
                    {% endif %}
                    {% endfor %}
                    {% endif %}
                    {% endfor %}
                    <h6 class="text-body">La categoría cuenta con {{ cuenta }} trámites en el catálogo.</h6>
                </div>
